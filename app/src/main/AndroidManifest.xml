<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gameapptask">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_gameimg"
        android:label="@string/disp_name"
        android:roundIcon="@mipmap/ic_gameimg_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".History"></activity>
        <activity android:name=".SummaryReport" />
        <activity android:name=".SecondPage" />
        <activity android:name=".PageOne" />
        <activity android:name=".NamePage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>